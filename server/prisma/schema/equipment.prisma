model Equipment {
  id          String   @id @default(uuid())
  name        String   @unique
  description String?
  imageUrl    String?
  createdBy   String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  admin User @relation("AdminEquipment", fields: [createdBy], references: [id])

  gyms GymEquipment[]

  @@map("equipment")
}
