model GymEquipment {
  gymId       String
  equipmentId String
  quantity    Int      @default(1) // Cantidad de esta m치quina en este gym
  assignedAt  DateTime @default(now()) // Opcional: cu치ndo se a침adi칩

  // Relaciones
  gym         Gym      @relation(fields: [gymId], references: [id], onDelete: Cascade)
  equipment   Equipment @relation(fields: [equipmentId], references: [id], onDelete: Cascade)

  @@id([gymId, equipmentId]) // Clave primaria compuesta
  @@map("gym_equipment")
}