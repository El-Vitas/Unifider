model Location {
    id          String   @id @default(uuid())
    name        String   @unique
    description String
    createdBy   String 
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    admin User @relation("AdminLocations", fields: [createdBy], references: [id])

    sectionTimeSlots SectionTimeSlot[] 
    gyms             Gym[] 
    courts           Court[]

    @@map("locations")
}
